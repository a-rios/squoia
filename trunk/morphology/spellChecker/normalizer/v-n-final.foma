# nominalizing suffixes that do not allow for successive verbablization (converbs), may only be followed by possessive and amb. suffixes

define VtoNfinal [
  [  {pti}  "+DS"  ] : {pti}"@PTI" 
| [  {pti}  "+DS"  ] : {qti}"@PTI"
| [  {stin} "+SS_Sim"  ] : {stin}
| [  {stin} "+SS_Sim"  ] : {sti}
| [  {spa}  "+SS"  ] : {spa}
| [  {spa}  "+SS"  ] : {shpa}
] ;
