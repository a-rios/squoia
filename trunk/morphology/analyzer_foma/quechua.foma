define EVID5  [~$["+IndE"?*["+DirE_Emph"|"+Asmp_Emph"]]];
define EVID6  [~$["+Asmp"?*["+IndE_Emph"|"+DirE_Emph"]]];
define EVID7 EVID4 .o. EVID5 .o. EVID6;

define NTAWAN [~$["+Proloc""+Con_Inst"]];
define ASWAN [~$["@AS""+Con_Inst"]];
# special case, 'aswan' as an entry of its own in the lexicon, as unit, because otherwise there would be conflicting suffix sequences, e.g. as-wan-ta!

define ALLIN [~$["@ALLI"["+3.Sg.Poss"|"+DirE"]]];
# special case, because both 'allin' and 'alli' do ocurr as roots

define HATUN1 [~$["@HATU""+3.Sg.Poss"]];
define HATUN2 [~$["@HATU""+DirE"]];
define HATUN3 [~$["@HATU"["+Asmp"|"+Asmp_Emph"]]];
define HATUN4 [~$["@HATU""+Dim"]];
define HATUN5 [~$["@HATU""+Fact"]];
define HATUN HATUN1 .o. HATUN2 .o. HATUN3 .o. HATUN4 .o. HATUN5;
# special case, because both 'hatun' and 'hatu' do ocurr as roots, additionally 'hatucha'=abuela, allow for verbalization only 'hatun-cha-'

define NAWPAQ1 [~$["@NAWPA""+Gen"]];
define NAWPAQ2 [~$["@NAWPA""+Ag"]];
define NAWPAQ NAWPAQ1 .o. NAWPAQ2;

# Feature restriction on lower side, can only be applied AFTER genitive rule (see section on nominal morpho-phonology)
define TAQ  [~$[{ta}"@TA"{q}"@PA"]];
define PAQ [~$[{pa}"@PA"{q}"@PA"]];
define STIN [~$["@STI"{n}]]; # -stin, not be -sti -n (3.Sg.Subj or DirE)
define PTIN [~$["@PTI"{n}"@MI"]]; #-pti -> only in Ayacuchano, -n as DirE only in Cusqueno, -pti-n can not be +DS+DirE
define FeatRestrLower  TAQ .o. PAQ .o. STIN .o. PTIN;

define FeatRestr [NAYA .o. SUN .o. SUNKI  .o. SUNCHIS .o. SUNKICHIS .o. WASUN1 .o. WASUN2 .o. WANQAKU .o. WANKU.o. TAPI .o. PITA .o. MANTA .o. NYARI .o. QTI .o. SQAN .o. SQAMA .o. YMAN  .o. TAMANTA .o. NPAQ .o. YKACHA  .o. NTIN .o. YUQ .o. TAQ .o. DISC  .o. tripleGEN.o. NTAWAN .o. ASWAN .o. ALLIN .o. HATUN .o. NAWPAQ .o. EVID7];

######################################################################################

# the only irregular verb form, imperative of 'hamuy' - to come

define HAKU  "VRoot+2.Sg.Imp" : {haku};

######################################################################################

define Word [[Noun|Verb|Prn|Particles|HAKU] (AS)] .o. NounRules  .o. AmbiRules .o. UpperCase  .o. Apo;

read regex Cleanup2 .o. FeatRestr .o. [Word|SonderWord] .o. FeatRestrLower .o. Cleanup1 .o. QuechuaOrtho .o. UpperCase;
#read regex  FeatRestr .o. [Word|SonderWord] .o. FeatRestrLower .o. Cleanup1 .o. QuechuaOrtho;
#read regex Cleanup2 .o. FeatRestr .o. [Word|SonderWord];

write att quechua-with-translations.att
clear
read att quechua-with-translations.att
