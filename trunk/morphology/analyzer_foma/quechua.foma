       define Orthography4 [ {w} (->) {u} || {a}_ ];
       define Orthography5 [ {h} (->) {j}];
       define Orthography6 [ {ch} (->) {ts}];
       define Orthography7 [ {a} (->) {á},{á} (->) {a} ,{e} (->) {é},{i} (->) {í}, {o} (->) {ó},{u} (->) {ú}];
