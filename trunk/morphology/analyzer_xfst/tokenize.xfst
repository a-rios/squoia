# Simple Tokenizer for Quechua 

define WS [" "|"\t"|"\n"];

define SYMBOL [";"|%,|"."|%-|":"|"["|"]"|"("|")"|"?"|%"|"¡"|"–"|"¿"|"!"|"/"];

define ALPHANUM [\ [WS |SYMBOL]];

define TOKEN [SYMBOL | ALPHANUM+];

define NormalizeSpace [ WS+ @-> "\n" ];

define Tokenizer [
	[TOKEN @-> ... "\n"]
	.o. NormalizeSpace
];

read regex Tokenizer.i ;
save stack tokenize.fst
